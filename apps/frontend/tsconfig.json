{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"jsx": "preserve",
		"allowJs": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"isolatedModules": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
